{
  "name": "jrisner",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "astro build",
    "clean": "node ./lib/clean.js",
    "dev": "astro dev",
    "format": "prettier . --list-different",
    "format:ci": "prettier . --check --cache",
    "lint:js": "eslint \"./**/*.{js,cjs,mjs}\"",
    "lint:md": "markdownlint \"**/*.md\" --ignore node_modules --disable MD041 MD013",
    "preview": "yarn build && astro preview",
    "test": "playwright test",
    "test:report": "playwright show-report ./specs/report",
    "test:ui": "playwright test --ui",
    "typecheck": "astro check && tsc --noEmit"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^1.3.3",
    "@astrojs/tailwind": "^4.0.0",
    "@playwright/test": "^1.35.1",
    "astro": "^2.8.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "markdownlint-cli": "^0.35.0",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2"
  }
}
